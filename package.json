  },
  "homepage": "https://github.com/cheminfo/eslint-config-cheminfo-react#readme",
  "dependencies": {
    "eslint-config-cheminfo": "^11.0.4",
    "eslint-plugin-react": "^7.35.0",
    "eslint-plugin-react-hooks": "^4.6.2",
    "eslint-plugin-react-refresh": "^0.4.8",
    "globals": "^15.8.0"
  },
  "peerDependencies": {
    "eslint": "^8.57.0"
